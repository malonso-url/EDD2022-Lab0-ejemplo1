
@{
    ViewData["Title"] = "HelloWorld";
}

<h1>Bienvenidos a EDD2022</h1>
<p>
    @ViewBag.greetings
</p>
<table class="table table-bordered table-striped table-responsive">
    <tr>
        <th>
            ID
        </th>
        <th>
            Nombre
        </th>
        <th>
            Apellido
        </th>
    </tr>
    <tr>
        <td>
            @ViewBag.id
        </td>
        <td>
            @ViewBag.firstname
        </td>
        <td>
            @ViewBag.lastname
        </td>
    </tr>
</table>


